{
  "name": "what2eat",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "create-env": "cp .env.base .env",
    "start-mongo": "mongod --pidfilepath mongo.pid --dbpath db > mongo.log &",
    "stop-mongo": "kill -TERM $(cat mongo.pid) && rm mongo.pid",
    "start": "node ./bin/www",
    "start-dev": "nodemon ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "mongoose": "^5.3.4",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.1.1"
  }
}
